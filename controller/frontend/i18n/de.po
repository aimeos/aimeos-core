#: src/Controller/Frontend/Basket/Default.php:411
#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "Das Produkt mit der ID \"%1$s\" darf nicht direkt zum Warenkorb hinzugefügt werden"

#: src/Controller/Frontend/Abstract.php:57
#: src/Controller/Frontend/Abstract.php:72
#, php-format
msgid "An empty domain is invalid"
msgstr "Für die Erzeugung eines Managers ist eine Domäne zwingend erforderlich"

#: src/Controller/Frontend/Basket/Default.php:598
#, php-format
msgid ""
"Available attribute IDs \"%1$s\" do not match the given attribute IDs "
"\"%2$s\""
msgstr "Die vorhandenden Attribute mit den IDs \"%1$s\" stimmen nicht im den übergebenen (\"%2$s\") überein"

#: src/Controller/Frontend/Basket/Default.php:214
#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "Das Produkt an Position \"%1$d\" im Warenkorb darf nicht verändert werden"

#: src/Controller/Frontend/Basket/Default.php:192
#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "Das Produkt an Position \"%1$d\" im Warenkorb kann nicht gelöscht werden"

#: src/Controller/Frontend/Common/Factory/Abstract.php:44
#: src/Controller/Frontend/Common/Factory/Abstract.php:117
#, php-format
msgid "Class \"%1$s\" not available"
msgstr "Klasse \"%1$s\" nicht verfügbar"

#: src/Controller/Frontend/Basket/Default.php:448
#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "Das Produkt mit der ID \"%2$s\" beinhaltet nicht die übergebenen \"%1$s\" Referenzen"

#: src/Controller/Frontend/Basket/Default.php:562
#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Ein oder mehrere Adressteile sind ungültig. Bitte überprüfen Sie Ihre Eingaben"

#: src/Controller/Frontend/Basket/Default.php:549
#, php-format
msgid "Invalid address property \"%1$s\" with value \"%2$s\""
msgstr "Der Adressteil \"%1$s\" mit dem Wert \"%2$s\" ist ungültig"

#: src/Controller/Frontend/Service/Factory.php:30
#: src/Controller/Frontend/Basket/Factory.php:31
#: src/Controller/Frontend/Common/Factory/Abstract.php:38
#: src/Controller/Frontend/Catalog/Factory.php:31
#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Klassenname \"%1$s\" enthält unerlaubte Zeichen"

#: src/Controller/Frontend/Abstract.php:67
#: src/Controller/Frontend/Common/Factory/Abstract.php:70
#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "Unbekannte Domäne \"%1$s\""

#: src/Controller/Frontend/Basket/Default.php:322
#, php-format
msgid "Invalid value for address type \"%1$s\""
msgstr "Es wurde kein gültiger Wert für den Adresstyp \"%1$s\" übergeben"

#: src/Controller/Frontend/Abstract.php:83
#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "Klasse \"%1$s\" implementiert nicht Interface \"%2$s\""

#: src/Controller/Frontend/Basket/Default.php:632
#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "Für die Domäne \"%1$s\" und den Code \"%2$s\" wurde kein Listentyp gefunden"

#: src/Controller/Frontend/Basket/Default.php:126
#, php-format
msgid "Product with ID \"%1$s\" and variant attribute IDs \"%2$s\" not available"
msgstr "Es ist kein Produkt mit der ID \"%1$s\" und den ausgewählten Varianten mit den IDs \"%2$s\" verfügbar"

#: src/Controller/Frontend/Service/Default.php:180
#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "Es wurde kein Eintrag für den Service mit der ID \"%2$s\" und dem Typ \"%1$s\" gefunden"

#: src/Controller/Frontend/Basket/Default.php:485
#, php-format
msgid "There are not enough products (ID \"%1$s\") in stock"
msgstr "Es sind leider nicht genügend von Ihnen gewünschte Produkte (ID \"%1$s\") vorhanden"

#: src/Controller/Frontend/Common/Decorator/Abstract.php:51
#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Die Methode \"%1$s::%2$s\" konnte nicht aufgerufen werden"

#: src/Controller/Frontend/Service/Default.php:82
#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "Es konnte kein Service \"%1$s\" mit der ID \"%2$s\" erstellt werden"

#: src/Controller/Frontend/Basket/Default.php:349
#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Die übergebenen Attribute \"%1$s\" konnten nicht zugeordnet werden"
