#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "No está permitido añadir un producto con la ID \"%1$s\""

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "Las ID de artículos disponibles \"%1$s\" no se corresponden con las ID de los atributos proporcionados  \"%2$s\""

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "En artículo de la cesta en la posición \"%1$d\" no puede modificarse"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "El artículo de la cesta en posición \"%1$d\" no puede borrarse manualmente"

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "La clase \"%1$s\" no implementa la interfaz \"%2$s\""

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "Clase \"%1$s\" no disponible"

#, php-format
msgid "Controller path is empty"
msgstr "La ruta del controlador está vacía"

#, php-format
msgid "Coupon code \"%1$s\" is invalid"
msgstr "El código de cupón \"%1$s\" no es válido"

#, php-format
msgid "Coupon code \"%1$s\" is invalid or not available any more"
msgstr "El código de cupón \"%1$s\" no es válido o ya no se encuentra disponible"

#, php-format
msgid "Coupon for code \"%1$s\" is not available any more"
msgstr "El código de cupón \"%1$s\" ya no se encuentra disponible"

#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "Referencias \"%1$s\" no válidas para el producto con la ID \"%2$s\""

#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Propiedades de dirección no válidas. Compruebe los datos introducidos"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Caracteres no válidos en el nombre de clase \"%1$s\""

#, php-format
msgid "Invalid characters in controller name \"%1$s\" in \"%2$s\""
msgstr "Caracteres no válidos en el nombre del controlador \"%1$s\" en \"%2$s\""

#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "Dominio \"%1$s\" no válido"

#, php-format
msgid "Invalid factory \"%1$s\""
msgstr "Constructor \"%1$s\" no válido "

#, php-format
msgid "Invalid value for address type \"%1$s\""
msgstr "Valor no válido para el tipo de dirección \"%1$s\""

#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "No se ha encontrado el tipo de lista para el dominio  \"%1$s\" y el código \"%2$s\""

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "No se han encontrado artículos para los atributos seleccionados y la ID de producto \"%1$s\""

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "No se ha encontrado ningún elemento para \"%1$s\" (%2$s)"

#, php-format
msgid "No stock for product ID \"%1$s\" and warehouse \"%2$s\" available"
msgstr "Ninguna existencia disponible para la ID de producto \"%1$s\" y el almacén \"%2$s\""

#, php-format
msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "No se ha encontrado un artículo único para los atributos seleccionados y la ID de producto \"%1$s\""

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "No se han cumplido los requisitos para el código de cupón \"%1$s\""

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "No se ha encontrado artículo de servicio para el tipo \"%1$s\" e ID \"%2$s\""

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "No hay suficientes existencias de los productos \"%1$s\""

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "No se pudo llamar al método \"%1$s::%2$s\""

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "No se pudo crear el proveedor \"%1$s\" para el servicio con la ID \"%2$s\""

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Atributos desconocidos \"%1$s\""
