#, php-format
msgid "Adding product with ID \"%1$s\" is not allowed"
msgstr "\"%1$s\" ID'li ürünü eklemeye izin verilmiyor"

#, php-format
msgid "Available attribute IDs \"%1$s\" do not match the given attribute IDs \"%2$s\""
msgstr "Mevcut nitelik ID'ler \"%1$s\" verilen nitelik ID'ler \"%2$s\" ile eşleşmiyor"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be changed"
msgstr "\"%1$d\" konumundaki sepet öğesi değiştirilemez"

#, php-format
msgid "Basket item at position \"%1$d\" cannot be deleted manually"
msgstr "\"%1$d\" konumundaki sepet öğesi manuel olarak silinemez"

#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "Class \"%1$s\" arayüz \"%2$s\" uygulamıyor"

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "Sınıf \"%1$s\" mevcut değil"

#, php-format
msgid "Controller path is empty"
msgstr "Denetleyici yolu boş"

#, php-format
msgid "Coupon code \"%1$s\" is invalid"
msgstr "\"%1$s\" kupon kodu geçersiz"

#, php-format
msgid "Coupon code \"%1$s\" is invalid or not available any more"
msgstr "\"%1$s\" kupon kodu geçersiz veya artık kullanılamaz"

#, php-format
msgid "Coupon for code \"%1$s\" is not available any more"
msgstr "\"%1$s\" kupon kodu artık kullanılamaz"

#, php-format
msgid "Invalid \"%1$s\" references for product with ID \"%2$s\""
msgstr "\"%2$s\" ID'li ürün için geçersiz \"%1$s\" referanslar"

#, php-format
msgid "Invalid address properties, please check your input"
msgstr "Geçersiz adres özellikleri, girişinizi kontrol edin"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "\"%1$s\" sınıf adında geçersiz karakterler"

#, php-format
msgid "Invalid characters in controller name \"%1$s\" in \"%2$s\""
msgstr " \"%2$s\" içindeki \"%1$s\" denetleyici adında geçersiz karakterler"

#, php-format
msgid "Invalid domain \"%1$s\""
msgstr "Geçersiz domain \"%1$s\""

#, php-format
msgid "Invalid factory \"%1$s\""
msgstr "Geçersiz fabrika \"%1$s\""

#, php-format
msgid "Invalid value for address type \"%1$s\""
msgstr "\"%1$s\" adres türü için geçersiz değer"

#, php-format
msgid "List type for domain \"%1$s\" and code \"%2$s\" not found"
msgstr "Domain \"%1$s\" ve kod \"%2$s\" için liste türü bulunamadı"

#, php-format
msgid "No article found for selected attributes and product ID \"%1$s\""
msgstr "Seçilen nitelikler ve \"%1$s\" ürün ID'si için makale bulunamadı"

#, php-format
msgid "No item for \"%1$s\" (%2$s) found"
msgstr "\"%1$s\" (%2$s) için hiçbir öğe bulumadı"

#, php-format
msgid "No stock for product ID \"%1$s\" and warehouse \"%2$s\" available"
msgstr "\"%1$s\" ürün ID'si ve \"%2$s\" depo için stok mevcut değil"

#, php-format
msgid "No unique article found for selected attributes and product ID \"%1$s\""
msgstr "Seçilmiş nitelikler ve \"%1$s\" ürün ID'si için benzersiz makale bulunamadı"

#, php-format
msgid "Requirements for coupon code \"%1$s\" aren't met"
msgstr "\"%1$s\" kupon kodu için gereksinimler karşılanmıyor"

#, php-format
msgid "Service item for type \"%1$s\" and ID \"%2$s\" not found"
msgstr "Tür \"%1$s\" ve ID \"%2$s\" ID için servis öğesi bulunamadı"

#, php-format
msgid "There are not enough products \"%1$s\" in stock"
msgstr "Stokta yeterli ürün \"%1$s\" yok"

#, php-format
msgid "Unable to call method \"%1$s::%2$s\""
msgstr "Metot \"%1$s::%2$s\" çağrılamıyor"

#, php-format
msgid "Unable to create provider \"%1$s\" for service with ID \"%2$s\""
msgstr "\"%2$s\" ID'li servis için sağlayıcı \"%1$s\" oluşturulamıyor"

#, php-format
msgid "Unknown attributes \"%1$s\""
msgstr "Bilinmeyen nitelikler \"%1$s\""
