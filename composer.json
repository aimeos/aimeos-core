{
	"name": "aimeos/aimeos-core",
	"description": "Full-featured e-commerce components for high performance online shops",
	"homepage": "https://aimeos.org",
	"license": "LGPL-3.0-or-later",
	"keywords": ["ecommerce", "shop", "portal", "marketplace", "gigacommerce", "B2B", "B2C"],
	"support": {
		"source": "https://github.com/aimeos/aimeos-core",
		"issues": "https://github.com/aimeos/aimeos-core/issues",
		"forum": "https://aimeos.org/help",
		"wiki": "https://aimeos.org/docs"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "^8.0.11",
		"composer-runtime-api": "^2.1",
		"ext-curl": "*",
		"ext-dom": "*",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pcre": "*",
		"ext-PDO": "*",
		"ext-zip": "*",
		"aimeos/aimeos-base": "2024.10.*",
		"aimeos/upscheme": "~1.0",
		"aimeos/macro": "~1.0",
		"aimeos/map": "^3.0.1",
		"nyholm/psr7": "~1.2",
		"psr/http-message": "~1.0||~2.0",
		"voku/portable-ascii": "~1.4||~2.0",
		"enshrined/svg-sanitize": "~0.18",
		"intervention/image": "^3.0"
	},
	"require-dev": {
		"php-coveralls/php-coveralls": "~2.0",
		"squizlabs/php_codesniffer": "~3.0",
		"phpunit/phpunit": "~10.0||~11.0",
		"phing/phing": "~2.4"
	},
	"autoload": {
		"psr-4": {
			"Aimeos\\": "src"
		},
		"classmap": [
			"Bootstrap.php",
			"Setup.php",
			"src"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Aimeos\\": "tests"
		},
		"classmap": [
			"tests"
		]
	}
}
