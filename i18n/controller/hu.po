#, php-format
msgid "Class \"%1$s\" does not implement interface \"%2$s\""
msgstr "A(z) \"%1$s\" osztály nem valósítja meg a(z) \"%2$s\" felület osztályt"

#, php-format
msgid "Class \"%1$s\" not available"
msgstr "A/Az \"%1$s\" osztály nem elérhető."

#, php-format
msgid "Class \"%1$s\" not found"
msgstr "A/Az \"%1$s\" osztály nem található."

#, php-format
msgid "Downloading file \"%1$s\" failed"
msgstr "\"%1$s\" állomány letöltése nem sikerült"

#, php-format
msgid "File \"%1$s\" not found"
msgstr "\"%1$s\" állomány nem található"

#, php-format
msgid "File type \"%1$s\" is not allowed"
msgstr "\"%1$s\" állomány nem engedélyezett"

#, php-format
msgid "Invalid characters in class name \"%1$s\""
msgstr "Érvénytelen karakter a/az \"%1$s\" osztály nevében"

#, php-format
msgid "No allowed image types configured for \"%1$s\""
msgstr "Nincs beállítva képformátum ehhez: \"%1$s\""

#, php-format
msgid "No type item for \"%1$s/%2$s\" in \"%3$s\" found"
msgstr "\"%1$s/%2$s\" számára a \"%3$s\"-ban/ben nincs fajtatétel"

#, php-format
msgid "Unable to create directory \"%1$s\""
msgstr "Nem sikerült létrehozni a könyvtárat \"%1$s\""

#, php-format
msgid "Unable to create file in \"%1$s\""
msgstr "Nem sikerült létrehozni a fájlt \"%1$s\""
